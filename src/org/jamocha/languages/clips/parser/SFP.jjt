SimpleNode Attributes() :
{} 
{ 
	(	LOOKAHEAD(2)
		Expression()
	)*
	{ return jjtThis; }
}



/**
*		Default Attribute
**/
//<default-attribute> ::= (default ?DERIVE | ?NONE | <expression>*) | (default-dynamic <expression>*)

SimpleNode DefaultAttribute() : 
{}
{
		<DEFAULT_ATR>
		(	
			DeriveAttribute()
		|	NoneAttribute() 
		|	DefaultAttributes() 
		)
		{ return jjtThis; }
}

SimpleNode DefaultAttributes() :
{}
{
