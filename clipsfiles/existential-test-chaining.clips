(watch all)
(deftemplate t1 (slot s1 (type INTEGER)))
(deftemplate t2 (slot s1 (type INTEGER)))
(defrule r1 (t1 (s1 ?x&9)) (exists (and (initial-fact) (t2))) => (assert (t1 (s1 2))))
(defrule r2 (t1 (s1 ?x&2)) => (printout t "x: " ?x crlf))
(assert (t1 (s1 5)))
(assert (t1 (s1 9)))
(run 1)
(assert (t2 (s1 100000)))
(run)
